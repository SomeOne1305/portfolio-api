{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "public": true,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "src/main.ts"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    }
  ],
  "installCommand": "pnpm install"
}
